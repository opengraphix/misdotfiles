# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(undo-preview-size small)
(monitor-xresolution 72.000000)
(monitor-yresolution 72.000000)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (padding-mode custom)
    (padding-color (color-rgb 0.360784 0.357955 0.357955)))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides no)
    (show-grid no)
    (show-sample-points yes)
    (padding-mode custom)
    (padding-color (color-rgb 0.427451 0.427451 0.427451)))
(default-snap-to-grid yes)
(default-snap-to-canvas yes)
(save-device-status yes)
(save-tool-options yes)
(theme "Gimp-CS6-Theme")


# end of gimprc
